<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Useractivity">

    <ImageView
        android:id="@+id/down"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="158dp"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/contact"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/ann"
        android:layout_marginEnd="39dp"
        app:srcCompat="@drawable/messa" />

    <ImageView
        android:id="@+id/pro"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_alignStart="@+id/contact"
        android:layout_alignTop="@+id/app"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/app"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="79dp"
        app:srcCompat="@drawable/apply" />

    <ImageView
        android:id="@+id/ann"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/app"
        android:layout_marginTop="272dp"
        app:srcCompat="@drawable/ann" />
</RelativeLayout>